{
  "identifier": "default",
  "description": "Default capabilities",
  "local": true,
  "windows": ["main"],
  "permissions": [
    "shell:allow-open",
    "core:default",
    "dialog:default",
    {
      "identifier": "shell:allow-spawn",
      "allow": [
        { "name": "bin/engine", "sidecar": true }
      ]
    },
    "updater:default",
    "updater:allow-check",
    "updater:allow-download-and-install"
  ]
}

